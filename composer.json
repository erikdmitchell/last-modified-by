{
    "name": "erikdmitchell/last-modified-by",
    "description": "Adds a modified by column to the admin columns",
    "type": "project",
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "wp-coding-standards/wpcs": "^3.2",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "phpcompatibility/php-compatibility": "^10.1"       
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Erik",
            "email": "erikdmitchell@gmail.com"
        }
    ],
    "scripts": {
        "phpcs": "phpcs -s -p --standard=./phpcs.ruleset.xml",
        "phpcbf": "phpcbf -p --standard=./phpcs.ruleset.xml"
    },
    "minimum-stability": "dev",
	"prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
